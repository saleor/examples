{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "saleor-app-authorize-net",
  "description": "Saleor App Authorize.net",
  "version": "0.4.3",
  "license": "(BSD-3-Clause AND CC-BY-4.0)",
  "private": true,
  "repository": "github:saleor/saleor-app-payment-template",
  "homepage": "https://github.com/saleor/saleor-app-payment-template",
  "bugs": {
    "url": "https://github.com/saleor/saleor-app-payment-template/issues",
    "email": "hello@saleor.io"
  },
  "type": "module",
  "scripts": {
    "dev": "pnpm generate && next dev --port 8000",
    "build": "pnpm generate && next build",
    "deploy": "tsx ./src/deploy.ts",
    "start": "next start",
    "fetch-schema": "curl https://raw.githubusercontent.com/saleor/saleor/${npm_package_saleor_schemaVersion}/saleor/graphql/schema.graphql > graphql/schema.graphql",
    "generate": "pnpm generate:graphql && pnpm generate:schema",
    "generate:graphql": "graphql-codegen",
    "generate:schema": "pnpm saleor-schema-compiler compile ./src/schemas --definitions ./src/schemas/definitions.json",
    "lint": "next lint --fix --dir src",
    "lint:ci": "next lint --dir src",
    "test": "vitest",
    "test:record": "POLLY_MODE=record_missing vitest",
    "test:rerecord": "POLLY_MODE=record vitest",
    "test:ci": "CI=true vitest --coverage --reporter=json --reporter=default && ./fix-coverage-report.cjs",
    "migrate": "pnpm tsx ./src/run-migrations.ts",
    "ts-node-esm": "node --loader ts-node/esm --experimental-specifier-resolution=node",
    "prepare": "husky install",
    "github:release": "pnpm changeset tag && git push --follow-tags"
  },
  "saleor": {
    "schemaVersion": "3.13"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.0",
    "@next/env": "13.4.19",
    "@radix-ui/react-alert-dialog": "1.0.4",
    "@saleor/app-sdk": "0.43.0",
    "@saleor/macaw-ui": "0.8.0-pre.123",
    "@sentry/nextjs": "7.80.0",
    "@t3-oss/env-nextjs": "0.6.1",
    "@tanstack/react-query": "4.33.0",
    "@trpc/client": "10.37.1",
    "@trpc/next": "10.37.1",
    "@trpc/react-query": "10.37.1",
    "@trpc/server": "10.37.1",
    "@urql/exchange-auth": "1.0.0",
    "@vanilla-extract/css": "1.13.0",
    "@vanilla-extract/next-plugin": "2.3.0",
    "@vanilla-extract/recipes": "0.5.0",
    "@vitejs/plugin-react": "4.0.4",
    "ajv": "8.12.0",
    "ajv-formats": "2.1.1",
    "authorizenet": "1.0.8",
    "bluebird": "3.7.2",
    "classnames": "2.3.2",
    "eslint-plugin-node": "11.1.0",
    "graphql": "16.8.1",
    "graphql-tag": "2.12.6",
    "jose": "4.14.4",
    "jsdom": "22.1.0",
    "lodash-es": "4.17.21",
    "micro": "10.0.1",
    "modern-errors": "6.0.0",
    "modern-errors-http": "4.0.0",
    "modern-errors-serialize": "5.0.0",
    "next": "14.2.3",
    "omit-deep-lodash": "1.1.7",
    "pino": "8.14.1",
    "pino-pretty": "10.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.45.4",
    "semver": "7.5.4",
    "tsx": "3.12.7",
    "url-join": "5.0.0",
    "urql": "3.0.4",
    "uuidv7": "0.6.2",
    "vite": "4.4.9",
    "vitest": "0.34.2",
    "webpack": "5.88.2",
    "yup": "1.2.0",
    "zod": "3.22.4",
    "zustand": "4.4.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
